<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Due Date</th>
        <th>completed</th>
      </tr>
<% for(var i=0;i< allTodos.length;i++){%>
    
        <tr>
            <td><%= allTodos[i].id %></td>
            <td><%= allTodos[i].title %></td>
            <td><%= allTodos[i].duedate %></td>
            <td><%= allTodos[i].markAsComplete %></td>
        </tr><br>
    </table>
    <%}%>
</table>
